        "ejs": "3.1.8",
        "express": "4.17.3",
        "express-winston": "2.6.0",
        "file-url": "3.0.0",
        "fs-extra": "10.1.0",
        "handlebars": "4.7.7",
        "i18n": "0.14.2",
        "jsonwebtoken": "8.5.1",
        "locale": "0.1.0",
        "moment": "^2.29.4",
        "mongodb": "3.6.6",
        "multer": "1.4.4",
        "puppeteer": "13.5.2",
        "sanitize-filename": "1.6.3",
        "winston": "2.4.4"
