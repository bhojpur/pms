{
    "name": "pdfgenerator",
    "version": "1.0.0",
    "description": "A generator or PDF documents (letters, contracts, invoices) for Bhojpur PMS application",
    "homepage": "https://www.bhojpur-consulting.com",
    "author": "Bhojpur Consulting Private Limited, India",
    "license": "MIT",
    "main": "index.js",
    "private": true,
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
